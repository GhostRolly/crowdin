{# templates/registration/register.html.twig #}
{% extends 'base.html.twig' %}
{% block body %}
{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.UserEmail) }}
    {{ form_row(form.Password.first) }}
    {{ form_row(form.Password.second) }}

    <button type="submit">Register!</button>

{{ form_end(form) }}

{% endblock %}